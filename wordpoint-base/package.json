{
  "name": "wordpoint-base",
  "version": "1.0.0",
  "description": "WordPoint Theme for WordPress",
  "homepage": "https://www.yurjo.com",
  "repository": {
    "type": "git",
    "url": "https://project-git-repo.tld"
  },
  "author": {
    "name": "Yanike Mann",
    "email": "support@yurjo.com",
    "url": "https://www.yurjo.com",
    "role": "developer"
  },
  "scripts": {
    "start": "npm install && composer install && gulp --gulpfile build/gulpfile.babel.js",
    "watch": "gulp --gulpfile build/gulpfile.babel.js watch",
    "build": "gulp --gulpfile build/gulpfile.babel.js",
    "dev": "npm install && composer install && gulp --gulpfile build/gulpfile.babel.js",
    "deploy": "npm install && composer install --no-dev -o && gulp --gulpfile build/gulpfile.babel.js",
    "lint-js": "eslint assets/js",
    "format-js": "eslint --fix assets/js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "eslintIgnore": [
    "assets/js/_vendor",
    "assets/js/admin/_vendor",
    "assets/js/frontend/_vendor",
    "assets/js/shared/_vendor"
  ],
  "lint-staged": {
    "*.js": [
      "eslint"
    ]
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.51",
    "@babel/preset-env": "^7.0.0-beta.51",
    "@babel/register": "^7.0.0-beta.51",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^8.0.0-beta",
    "browserslist": "^3.2.8",
    "caniuse-db": "^1.0.30000853",
    "dateformat": "^3.0.3",
    "del": "^3.0.0",
    "eslint": "^4.18.2",
    "eslint-config-wordpress": "^2.0.0",
    "eslint-loader": "^2.0.0",
    "gulp": "^4.0.0",
    "gulp-cache": "^1.0.2",
    "gulp-cssnano": "^2.1.2",
    "gulp-filter": "^5.1.0",
    "gulp-imagemin": "^4.1.0",
    "gulp-livereload": "^3.8.1",
    "gulp-notify": "^3.0.0",
    "gulp-postcss": "^7.0.1",
    "gulp-rename": "^1.3.0",
    "gulp-sass": "^4.0.1",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-zip": "^4.1.0",
    "husky": "^1.0.0-rc.7",
    "lint-staged": "^7.1.2",
    "node-sass-tilde-importer": "^1.0.2",
    "postcss-import": "^10.0.0",
    "postcss-preset-env": "^5.1.0",
    "pump": "^1.0.3",
    "require-dir": "^0.3.2",
    "stylelint": "^9.2.1",
    "stylelint-config-wordpress": "^13.0.0",
    "webpack": "^4.12.0",
    "webpack-cli": "^2.1.5",
    "webpack-stream": "^3.2.0"
  },
  "dependencies": {
    "foundation-sites": "^6.4.4-rc1",
    "motion-ui": "^2.0.2",
    "slick-carousel": "^1.8.1",
    "what-input": "^5.0.5"
  },
  "browserslist": [
    "> 1%",
    "Explorer 11",
    "last 2 versions",
    "safari >= 7"
  ],
  "engineStrict": true,
  "engines": {
    "node": "~8.11"
  }
}
